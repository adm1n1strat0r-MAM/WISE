<body>


<div style="justify-content: center; display: flex; align-items: center; margin-top: 3rem;">
<div style="width:800px; height:600px; padding:20px; text-align:center; border: 10px solid #787878; background-color: #550e1f; color: white;">
	<div style="width:750px; height:550px; padding:20px; text-align:center; border: 5px solid #787878">
		   <span style="font-size:50px; font-weight:bold">Certificate of Completion</span>
		   <br><br>
		   <span style="font-size:25px"><i>This is to certify that</i></span>
		   <br><br>
		   <span style="font-size:30px"><b><%=data.name%></b></span><br/><br/>
		   <span style="font-size:25px"><i>has completed the course</i></span> <br/><br/>
		   <span style="font-size:30px"><%=data.selectedValue%></span> <br/><br/>
		   <span style="font-size:20px">with score of <b><%=data.percentage%>%</b></span> <br/><br/><br/>
		   <span style="font-size:16px"><i>max price you can set on your gig: </i></span> 
		   <%
			let maxPrice = 0;
			if (data.percentage > 50 && data.percentage < 70) {
				maxPrice = 50;
			} else if (data.percentage >= 70 && data.percentage < 80) {
				maxPrice = 200;
			} else if (data.percentage >= 80 && data.percentage < 90) {
				maxPrice = 500;
			} else if (data.percentage >= 90) {
				maxPrice = 1000;
			}
			%>
			<%=maxPrice%>$
		   <br/>
		   <span style="font-size:22px"><i>dated: </i></span>
		   <%=data.date%>
		   
	</div>
	</div>
</div>

<div style="justify-content: center; display: flex; align-items: center; margin-top: 1rem;">
<a href="http://localhost:4000/seller/<%=data.selectedValue%>" type="button" id="next"  class="btn btn-primary" >NEXT</a>
</div>
  </body>

  </html>
